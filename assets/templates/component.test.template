describe("llq-{componentNameKebabCased}", () => {
    let component: {namespace}.{className}Controller;
    let $componentController: ng.IComponentController;
    let scope: ng.IScope;

    beforeEach(angular.mock.module({appname}));

    beforeEach(inject((
        _$componentController_: ng.IComponentController,
        _$rootScope_: ng.IRootScopeService
        ) => {
        $componentController = _$componentController_;
        $rootScope = _$rootScope_;
        scope = $rootScope.$new();
    }));

    describe("componentName", () => {
        it("should return llq{className}", () => {
            let result = {namespace}.{className}Controller.componentName;
            expect(result).toBe("llq{className}");
        });
    });
})