{
  "name": "vscode-angular1-component-generator",
  "displayName": "Angular 1.5 Component Generator",
  "description": "Generates angular 1.5(or up) component files (html, css, typescript -> template, stylesheet, component.ts + module.ts)",
  "version": "0.0.1",
  "publisher": "xsintill",
  "author": {
    "name": "Luc Neville",
    "email": "lneville@gmail.com"
  },
  "homepage": "https://github.com/xsintill/vscode-angular1-component-generator",
  "galleryBanner": {
      "color": "#0273D4",
      "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xsintill/vscode-angular1-component-generator.git"
  },
  "bugs": {
    "url": "https://github.com/xsintill/vscode-angular1-component-generator/issues",
    "email": "lneville@gmail.com"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "vscode-angular1",
    "angular1",
    "angular1 component",
    "angular1 generate component",
    "typescript"
  ],
  "activationEvents": [
    "onCommand:extension.genAngular1ComponentFiles"
  ],
  "main": "./out/src/extension",
  "icon": "assets/images/icon.png",
  "contributes": {
    "commands": [
      {
        "command": "extension.genAngular1ComponentFiles",
        "title": "New Angular1 Component"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.genAngular1ComponentFiles",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "change-case": "^3.0.0",
    "fs": "0.0.1-security",
    "fs-extra": "^0.30.0",
    "rxjs": "^5.0.0-rc.1"
  }
}
